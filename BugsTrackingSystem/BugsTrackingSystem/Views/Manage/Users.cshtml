@model BugsTrackingSystem.Models.ProjectExtendedViewModel

@{
    ViewBag.Title = "Users";
}

<div class="container">
    <div class="col-md-12 col-xs-12">
        <ul class="list-inline">
            <li>
                <h1><b>Users</b>
                </h1>
            </li>
            <li>
                <a href="#"><i class="fa fa-plus-circle projPlus" aria-hidden="true"></i></a>
            </li>
        </ul>

        @foreach (var user in Model.Users)
        {
            <a href="@Url.Action("Project", "Manage")" class="projectsPage">
                <div class="col-md-3 col-xs-6 MyProjects">
                    <div class="col-md-3">
                        <img class="img-circle" src="~/images/orwell.jpg" width="50" height="50"/>
                    </div>
                    <div class="col-md-8">
                        <span>George Orwell</span><br>
                        <span>george@@email.com</span>
                    </div>
                    <br><br><br>
                    <div class="col-md-3">
                        <h5><span class="project_progress">Me</span></h5>
                    </div>
                    <div class="text-right">
                        <ul class="list-inline icons">
                            <li>
                                <i class="fa fa-pencil-square-o" aria-hidden="true"> 13</i>
                            </li>
                            <li>
                                <i class="fa fa-list" aria-hidden="true"> 3</i>
                            </li>
                        </ul>
                    </div>
                </div>
            </a>
        }
       
    </div>
   
</div>
<div class="container">
    <i class="fa fa-users" aria-hidden="true"> Users: @Html.Encode(Model.Users)</i>
</div>
